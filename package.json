{
  "name": "snekdocs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "ts-node --project tsconfig.server.json src/server/server.ts ",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext js,jsx,ts,tsx",
    "test": "jest --watchAll",
    "test:unit": "jest",
    "test:e2e-ci": "",
    "test:coverage": "jest --coverage",
    "createDevDatabase": "docker-compose up -d",
    "db:studio": "prisma studio --schema ./src/prisma/schema.prisma",
    "db:migrate": "prisma migrate dev --schema ./src/prisma/schema.prisma",
    "db:reseed": "prisma migrate reset --preview-feature --schema ./src/prisma/schema.prisma",
    "db:seed": "prisma db seed --preview-feature --schema ./src/prisma/schema.prisma",
    "jwt-proxy": "jwt-proxy 9000 3000 --silent --jwt user.jwt",
    "db:reset": "prisma db push --schema ./src/prisma/schema.prisma --force-reset --preview-feature && npm run db:seed",
    "db:generate": "prisma generate --schema ./src/prisma/schema.prisma"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@prisma/client": "^2.21.2",
    "@types/dom-to-image": "^2.6.2",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.41",
    "@types/quill": "^2.0.6",
    "@types/react": "^17.0.3",
    "@types/socket.io": "^2.1.13",
    "@types/styled-components": "^5.1.9",
    "@types/uuid": "^8.3.0",
    "axios": "^0.21.1",
    "babel-plugin-macros": "^3.0.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-styled-components": "^1.12.0",
    "babel-plugin-superjson-next": "^0.2.3",
    "dom-to-image": "^2.6.0",
    "express": "^4.17.1",
    "lodash.debounce": "^4.0.8",
    "next": "10.0.5",
    "postcss": "^8.2.8",
    "prisma": "^2.21.2",
    "quill": "^1.3.7",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-error-boundary": "^3.1.1",
    "react-query": "^3.13.10",
    "socket.io": "^4.0.2",
    "socket.io-client": "^4.0.2",
    "styled-components": "^5.2.1",
    "tailwindcss": "^2.0.4",
    "ts-node-dev": "^1.1.6",
    "twin.macro": "^2.3.0",
    "typeface-dm-sans": "^1.1.13",
    "typescript": "^4.2.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@testing-library/dom": "^7.29.4",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.5",
    "@testing-library/react-hooks": "^5.1.0",
    "@testing-library/user-event": "^12.6.3",
    "@trongnd/ts-nodemon": "^1.0.3",
    "@types/express": "^4.17.11",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.22.0",
    "eslint-import-resolver-babel-module": "^5.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.8",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "msw": "^0.27.2",
    "node-mocks-http": "^1.10.1",
    "nodemon": "^2.0.7",
    "prettier": "2.2.1",
    "pretty-quick": "^3.1.0",
    "react-is": "^17.0.1",
    "ts-node": "^9.1.1"
  },
  "babelMacros": {
    "twin": {
      "preset": "styled-components",
      "config": "./tailwind.config.js"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
